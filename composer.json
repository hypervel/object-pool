{
    "name": "laravel-hyperf/object-pool",
    "description": "The object pool package for Laravel Hyperf.",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf",
        "pool",
        "object-pool",
        "swoole",
        "laravel-hyperf"
    ],
    "support": {
        "issues": "https://github.com/laravel-hyperf/components/issues",
        "source": "https://github.com/laravel-hyperf/components"
    },
    "authors": [
        {
            "name": "Albert Chen",
            "email": "albert@laravel-hyperf.com",
            "homepage": "https://albert-chen.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "hyperf/coroutine": "~3.1.0",
        "hyperf/engine": "^2.1",
        "hyperf/coordinator": "~3.1.0",
        "hyperf/contract": "~3.1.0",
        "hyperf/support": "~3.1.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelHyperf\\ObjectPool\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.1-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}