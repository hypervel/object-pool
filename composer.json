{
    "name": "swooletw/hyperf-object-pool",
    "description": "The object pool package for Hyperf.",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf",
        "pool",
        "object-pool",
        "swoole"
    ],
    "support": {
        "issues": "https://github.com/swooletw/hyperf-packages/issues",
        "source": "https://github.com/swooletw/hyperf-packages"
    },
    "authors": [
        {
            "name": "Albert Chen",
            "email": "albert.cht@pm.me",
            "homepage": "https://albert-chen.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "hyperf/coroutine": "^3.1",
        "hyperf/engine": "^2.1",
        "hyperf/coordinator": "^3.1",
        "hyperf/contract": "^3.1",
        "hyperf/support": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "SwooleTW\\Hyperf\\ObjectPool\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.1-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}